{
  "i18n": true,
  "type": "panel",
  "items": {
    "apiType": {
      "sm": 5,
      "type": "select",
      "label": "Select API",
      "newLine": true,
      "options": [
        { "label": "Closed API", "value": 0 },
        { "label": "Open API", "value": 1 }
      ]
    },
    "username": {
      "sm": 5,
      "type": "text",
      "label": "Username",
      "newLine": true,
      "hidden": "data.apiType == 1"
    },
    "password": {
      "sm": 5,
      "type": "password",
      "label": "Password",
      "newLine": true,
      "hidden": "data.apiType == 1"
    },
    "appID": {
      "sm": 5,
      "type": "text",
      "label": "Personal APP ID",
      "newLine": true,
      "hidden": "data.apiType == 0"
    },
    "appSecret": {
      "sm": 5,
      "type": "password",
      "label": "Personal APP Secret",
      "newLine": true,
      "hidden": "data.apiType == 0"
    },
    "systemId": {
      "sm": 5,
      "type": "text",
      "label": "Alpha ESS System ID",
      "newLine": true
    },
    "enableRealtimedata": {
      "type": "checkbox",
      "newLine": true,
      "hidden": "data.apiType == 1"
    },
    "intervalRealtimedata": {
      "sm": 5,
      "min": 1,
      "type": "number",
      "label": "Interval to read realtime data",
      "newLine": false,
      "disabled": "!data.enableRealtimedata",
      "hidden": "data.apiType == 1"
    },
    "enableEnergydata": {
      "type": "checkbox",
      "newLine": true,
      "hidden": "data.apiType == 1"
    },
    "intervalEnergydataMins": {
      "sm": 5,
      "min": 1,
      "type": "number",
      "label": "Interval to read energy data",
      "newLine": false,
      "disabled": "!data.enableEnergydata",
      "hidden": "data.apiType == 1"
    },
    "enableSettingsdata": {
      "type": "checkbox",
      "newLine": true,
      "hidden": "data.apiType == 1"
    },
    "enableSettingsCharge": {
      "type": "checkbox",
      "newLine": true,
      "hidden": "true"
    },
    "intervalSettingsChargeMins": {
      "sm": 5,
      "min": 1,
      "type": "number",
      "label": "Interval to read charging settings",
      "newLine": false,
      "disabled": "!data.oAEnableSettingsCharge",
      "hidden": "true"
    },
    "enableSettingsDisCharge": {
      "type": "checkbox",
      "newLine": true,
      "hidden": "true"
