      "min": 10,
      "type": "number",
      "label": "Interval to read realtime data",
      "newLine": false,
      "disabled": "!data.oAEnableRealtime",
      "hidden": "data.apiType == 0"
    },
    "oAEnableEnergy": {
      "type": "checkbox",
      "newLine": true,
      "hidden": "data.apiType == 0"
